; Input variables
(declare-const X_0 Real)
(declare-const X_1 Real)

; Output variables
(declare-const Y_0 Real)
(declare-const Y_1 Real)

; Hidden variables
(declare-pwl N_1 ReLU)
(declare-pwl N_2 ReLU)
(declare-pwl N_3 ReLU)
(declare-pwl N_4 ReLU)

; Input constraints
(assert (or 
  (and (>= X_0 -2.0) (<= X_0  2.0) (>= X_1  0.0) (<= X_1  1.0))
  (and (>= X_0 -2.0) (<= X_0  2.0) (>= X_1 -1.0) (<= X_1  0.0))
))

; Output constraints
(assert (<= Y_0 Y_1))
